  },
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.5.11",
    "jshint-stylish": "^1.0.0",
    "nodemon": "^1.2.1"
  },
  "engines": {
    "node": ">=0.10"
  },
  "keywords": [
    "strkio"
  ]
}
